{
  "name": "antistasi-development",
  "displayName": "Antistasi Development",
  "description": "Utility to help Antistasi Devs",
  "author": {
    "name": "Giddi"
  },
  "license": "MIT",
  "repository": {},
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "Antistasi",
    "sqf",
    "Arma"
  ],
  "preview": true,
  "extensionKind": [],
  "pricing": "Free",
  "activationEvents": [
    "onLanguage:sqf",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "sqf",
        "path": "./src/snippets/sqf.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@types/xml2js": "^0.4.14",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  }
}