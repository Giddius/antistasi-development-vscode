{
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"baseUrl": "./src",
		"paths": {
			"typings": [
				"./typings/*"
			]
		},
		"resolvePackageJsonImports": true,
		"target": "ES2020",
		"outDir": "out",
		"lib": [
			"ES2020",
			"ES2021.WeakRef",
			"ES2022.RegExp"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"diagnostics": false,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"declaration": false
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"temp"
	]
}